{
    "name": "shadeup",
    "displayName": "Shadeup",
    "description": "Shadeup Syntax Highlighting",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "shadeup",
            "aliases": ["Shadeup", "shadeup"],
            "extensions": [".shadeup"],
            "configuration": "./language-configuration.json",
            "icon": {
                "light": "./icon.svg",
                "dark": "./icon.svg"
            }
        }],
        "grammars": [{
            "language": "shadeup",
            "scopeName": "source.shadeup",
            "path": "./syntaxes/shadeup.tmLanguage.json",
            "embeddedLanguages": {
                "meta.embedded.block.hlsl": "hlsl"
            }
        }]
    }
}